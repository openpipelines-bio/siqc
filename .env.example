# QC Report Generator Environment Variables
# Copy this file to .env and customize for your development setup

# Data source paths (no defaults - must be specified)
# QC_DATA_PATH=resources_test/xenium_dataset_large/data.json
# QC_STRUCTURE_PATH=resources_test/xenium_dataset_large/structure.json

# Payload cache location (optional)
# QC_PAYLOAD_PATH=qc-report-payload.bin

# Auto-generate payload from data files (default: true)
# QC_AUTO_GENERATE=true

# ================================
# Common Development Configurations
# ================================

# Small single-cell dataset (~4KB)
# QC_DATA_PATH=resources_test/sc_dataset_small/data.json
# QC_STRUCTURE_PATH=resources_test/sc_dataset_small/structure.json

# Large single-cell dataset (~68MB, 1.2M cells)
# QC_DATA_PATH=resources_test/sc_dataset_large/data.json
# QC_STRUCTURE_PATH=resources_test/sc_dataset_large/structure.json

# Small Xenium spatial dataset
# QC_DATA_PATH=resources_test/xenium_dataset_small/data.json
# QC_STRUCTURE_PATH=resources_test/xenium_dataset_small/structure.json

# Large Xenium spatial dataset (~100MB, 1.2M cells)
# QC_DATA_PATH=resources_test/xenium_dataset_large/data.json
# QC_STRUCTURE_PATH=resources_test/xenium_dataset_large/structure.json

# Progressive testing dataset
# QC_DATA_PATH=resources_test/progressive_test/data.json
# QC_STRUCTURE_PATH=resources_test/progressive_test/structure.json
